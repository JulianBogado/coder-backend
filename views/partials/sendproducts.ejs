<div class="container">
        <h3 class="text-primary">Ingrese un nuevo producto</h3>
        <form action="/" method="post">
                <div class="form-group"><label for="nombre" class="font-weight-bold">Titulo</label><input type="text"
                                name="title" class="form-control"></div>
                <div class="form-group"><label for="apellido" class="font-weight-bold">Url imagen</label><input
                                type="text" name="thumbnail" class="form-control"></div>
                <div class="form-group"><label for="edad" class="font-weight-bold">Precio</label><input type="price"
                                name="edad" class="form-control"></div><input type="submit" value="Enviar"
                        class="btn btn-success mt-3 mb-5">
        </form>
</div>