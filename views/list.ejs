

<div class="container">
    <h1> <%- title %></h1>
        <% if (data.length> 0) { %>
            <table class="table">
                <tr>
                    <th scope="col">Name</th>
                    <th scope="col">Price</th>
                    <th scope="col">Image</th>
                </tr>
                <% for(let producto of data) { %>
                    <tr>
                        <td>
                            <%=producto.title %>
                        </td>
                        <td>
                            <%=producto.price %>
                        </td>
                        <td>
                            <img src=<%=producto.thumbnail %> style="width: 50px;"></img>
                        </td>
                    </tr>
                    <% }%>
            </table>
            <% } else { %>
                <span>No Products to show</span>
                <% } %>

    </div>